<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zigzago">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icc_launcher"
        android:label="ZIGZAGO"
        android:roundIcon="@mipmap/icc_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.zigzago.Customerwishlist"></activity>
        <activity android:name="com.example.zigzago.rating" />
        <activity android:name="com.example.zigzago.Bill" />
        <activity android:name="com.example.zigzago.ReSet1_pass2" />
        <activity android:name="com.example.zigzago.pin_page2" />
        <activity android:name="com.example.zigzago.forget2" />
        <activity android:name="com.example.zigzago.recycleview2" />
        <activity
            android:name="com.example.zigzago.MenuActivity"
            android:label=""
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.zigzago.Log_in" />
        <activity
            android:name="com.example.zigzago.Sign_up"
            android:parentActivityName="com.example.zigzago.MainActivity" />
        <activity android:name="com.example.zigzago.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>